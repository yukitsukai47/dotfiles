syntax on
set tabstop=2
set shiftwidth=2
set expandtab
set ai
set smartindent
set number
set hlsearch
set ruler
set incsearch
set ignorecase
set smartcase
set nobackup
set noswapfile
set autoread
set hidden
set showcmd
set clipboard=unnamed,autoselect
set virtualedit=onemore
set laststatus=2

colorscheme peachpuff
nnoremap <C-j> 5j
noremap <C-k> 5k
highlight Comment ctermfg=green
